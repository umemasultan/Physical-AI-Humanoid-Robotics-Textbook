# Qdrant Cloud Configuration (FREE)
# Get from: https://cloud.qdrant.io
QDRANT_URL=https://your-cluster-id.qdrant.io
QDRANT_API_KEY=your-qdrant-api-key

# Groq LLM API (FREE - Fast inference)
# Get from: https://console.groq.com
GROQ_API_KEY=your-groq-api-key

# Embedding Model (runs locally)
EMBEDDING_MODEL=all-MiniLM-L6-v2

# CORS - Add your Vercel URL
CORS_ORIGINS=http://localhost:3000,https://physical-ai-humanoid-robotics-textbook.vercel.app

# Rate Limiting
RATE_LIMIT_PER_MINUTE=20

# Application
APP_ENV=production
LOG_LEVEL=INFO
